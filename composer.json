{
    "name": "voku/phpstan-rules",
    "description": "Provides additional rules for phpstan/phpstan.",
    "license": "MIT",
    "type": "phpstan-extension",
    "keywords": [
        "phpstan",
        "phpstan-rules",
        "phpstan-extreme-rules"
    ],
    "authors": [
        {
            "name": "Lars Moelleken",
            "email": "lars@moelleken.org"
        }
    ],
    "homepage": "https://github.com/voku/phpstan-rules",
    "support": {
        "issues": "https://github.com/voku/phpstan-rules/issues",
        "source": "https://github.com/voku/phpstan-rules"
    },
    "require": {
        "php": "^7.2 || ^8.0",
        "phpstan/phpstan": "^1.9.0"
    },
    "require-dev": {
        "phpstan/phpstan-phpunit": "^1.1.0",
        "phpunit/phpunit": "~7.0 || ~9.0"
    },
    "autoload": {
        "psr-4": {
            "voku\\PHPStan\\Rules\\": "src/voku/PHPStan/Rules/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "voku\\PHPStan\\Rules\\Test\\": "tests/"
        }
    },
    "extra": {
        "phpstan": {
            "includes": [
                "rules.neon"
            ]
        }
    }
}
